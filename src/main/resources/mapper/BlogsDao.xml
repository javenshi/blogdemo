<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.syj.blog.dao.BlogsDao">
    <select id="findAll" resultType="Blogs">
        select blogsid,blogsname,blogscontext,blogsstatus,blogsclick,blogsdate,blogsclassifyid,userid
        from blogs
